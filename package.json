{
  "name": "mixpanel-nuxt",
  "version": "0.0.15",
  "description": "Mixpanel module for Nuxt",
  "repository": "JoaoPedroAS51/nuxt-mixpanel",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "nuxt-module-build prepare",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:generate": "nuxi generate playground",
    "dev:preview": "nuxi preview playground",
    "dev:prepare": "pnpm prepare && pnpm build:stub",
    "build": "nuxt-module-build build",
    "build:stub": "pnpm build --stub",
    "prepack": "pnpm build",
    "release": "pnpm lint && pnpm test && pnpm prepack && pnpm changelogen --release --push && pnpm publish && git push --follow-tags",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "^4.2.1",
    "mixpanel-browser": "^2.72.0"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^1.10.0",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.2.1",
    "@nuxt/test-utils": "^3.20.1",
    "@types/node": "^24.10.1",
    "changelogen": "^0.6.2",
    "eslint": "^9.39.1",
    "nuxt": "^4.2.1",
    "vitest": "^4.0.9"
  }
}
